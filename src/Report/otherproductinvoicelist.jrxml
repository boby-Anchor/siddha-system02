	<parameter name="refno" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
     invoiceitem.`qty` AS invoiceitem_qty,
     invoiceitem.`sellprice` AS invoiceitem_sellprice,
     invoiceitem.`total` AS invoiceitem_total,
     item.`itemname` AS item_itemname,
     itemsubcatogery.`terms` AS itemsubcatogery_terms
FROM
     `stock` stock INNER JOIN `invoiceitem` invoiceitem ON stock.`idstock` = invoiceitem.`idstock`
     INNER JOIN `item` item ON stock.`iditem` = item.`iditem`
     INNER JOIN `itemsubcatogery` itemsubcatogery ON item.`iditemsubcatogery` = itemsubcatogery.`iditemsubcatogery`
     INNER JOIN `otherproduct` otherproduct ON invoiceitem.`idinvoice` = otherproduct.`idotherproduct`
WHERE
     idinvoice = $P{refno}]]>
	</queryString>
