	<queryString>
		<![CDATA[SELECT
     rx.`idrx` AS rx_idrx,
     rx.`iddoctor` AS rx_iddoctor,
     rx.`payment_type` AS rx_payment_type,
     rx.`billto` AS rx_billto,
     rx.`baddress` AS rx_baddress,
     rx.`bnumber` AS rx_bnumber,
     rx.`deliverTo` AS rx_deliverTo,
     rx.`daddress` AS rx_daddress,
     rx.`dnumber` AS rx_dnumber,
     invoicerx.`date` AS invoicerx_date,
     invoicerx.`time` AS invoicerx_time,
     invoicerx.`total` AS invoicerx_total,
     invoicerx.`discount` AS invoicerx_discount,
     invoicerx.`shippingcost` AS invoicerx_shippingcost,
     invoicerx.`fullamount` AS invoicerx_fullamount,
     rxitem.`itemname` AS rxitem_itemname,
     rxitem.`unitprice` AS rxitem_unitprice,
     rxitem.`qty` AS rxitem_qty,
     rxitem.`total` AS rxitem_total,
     rxitem.`terms` AS rxitem_terms
FROM
     `rx` rx INNER JOIN `invoicerx` invoicerx ON rx.`idrx` = invoicerx.`rx_idrx`
     INNER JOIN `rxitem` rxitem ON invoicerx.`rx_idrx` = rxitem.`rx_idrx`
WHERE
     rx.`idrx` = $P{RefNo}]]>
