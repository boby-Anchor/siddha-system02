	<queryString>
		<![CDATA[SELECT
     invoice.`date` AS invoice_date,
     invoice.`time` AS invoice_time,
     invoice.`total` AS invoice_total,
     invoice.`dsicount` AS invoice_dsicount,
     invoice.`shippingcost` AS invoice_shippingcost,
     invoice.`fullamount` AS invoice_fullamount,
     otherproduct.`idotherproduct` AS otherproduct_idotherproduct,
     otherproduct.`payment_type` AS otherproduct_payment_type,
     otherproduct.`billto` AS otherproduct_billto,
     otherproduct.`bnumber` AS otherproduct_bnumber,
     otherproduct.`baddress` AS otherproduct_baddress,
     otherproduct.`deliverto` AS otherproduct_deliverto,
     otherproduct.`dnumber` AS otherproduct_dnumber,
     otherproduct.`daddress` AS otherproduct_daddress
FROM
     `otherproduct` otherproduct INNER JOIN `invoice` invoice ON otherproduct.`idotherproduct` = invoice.`otherproduct_id`
WHERE
     otherproduct.`idotherproduct` = $P{RefNo}]]>
	</queryString>
